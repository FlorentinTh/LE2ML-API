{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@Server": "./src/server",
          "@Config": "./src/utils/config",
          "@Cache": "./src/utils/cache",
          "@Logger": "./src/utils/logger",
          "@Redis": "./src/utils/redis",
          "@Mongo": "./src/utils/mongo",
          "@Passport": "./src/utils/passport",
          "@Configuration": "./src/server/configuration/configuration",
          "@APIError": "./src/server/helpers/api.error",
          "@Authority": "./src/server/helpers/authority",
          "@FileHelper": "./src/server/helpers/file.helper",
          "@StreamHelper": "./src/server/helpers/stream.helper.js",
          "@StringHelper": "./src/server/helpers/string.helper.js"
        },
        "extensions": [".js", ".json"]
      }
    ]
  ]
}
