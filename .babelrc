{
	"presets": ["@babel/preset-env"],
	"plugins": [
		["@babel/transform-runtime"], ["module-resolver",
			{
				"root": ["."],
				"alias": {
					"@App": "./src/app",
					"@Config": "./src/utils/Config",
					"@Logger": "./src/utils/logger",
					"@Mongo": "./src/utils/Mongo",
					"@Passport": "./src/utils/passport",
					"@APIError": "./src/server/helpers/APIError"
				},
				"extensions": [".js", ".json"]
			}
		]
	]
}
