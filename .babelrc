{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@Server": "./src/server",
          "@APIError": "./src/utils/api.error",
          "@Authority": "./src/utils/authority",
          "@Config": "./src/utils/config",
          "@Cache": "./src/utils/cache",
          "@Logger": "./src/utils/logger",
          "@Redis": "./src/utils/redis",
          "@Mongo": "./src/utils/mongo",
          "@Passport": "./src/utils/passport",
          "@Configuration": "./src/server/configuration/configuration",
          "@ContainerHelper": "./src/helpers/container.helper",
          "@FileHelper": "./src/helpers/file.helper",
          "@JobLogsHelper": "./src/helpers/job.logs.helper.js",
          "@StreamHelper": "./src/helpers/stream.helper.js",
          "@StringHelper": "./src/helpers/string.helper.js"
        },
        "extensions": [".js", ".json"]
      }
    ]
  ]
}
