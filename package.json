{
  "name": "picluster-api",
  "version": "1.0.0",
  "description": "API for the PiCluster project",
  "main": "src/server.js",
  "scripts": {
    "lint": "eslint . --ext .js,.json",
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist",
    "public": "mkdirp dist/public && cpy src/public/ dist/public",
    "copy": "cpy .env dist",
    "start": "cross-env NODE_ENV=development npm-run-all development",
    "serve": "cross-env NODE_ENV=production npm-run-all clean build copy production",
    "development": "nodemon src/bin/www --exec babel-node --presets @babel/preset-env",
    "production": "node dist/bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorentinTh/PiCluster-API.git"
  },
  "author": "FlorentinTh",
  "license": "Apache-2.0",
  "pre-commit": [
    "lint"
  ],
  "bugs": {
    "url": "https://github.com/FlorentinTh/PiCluster-API/issues"
  },
  "homepage": "https://github.com/FlorentinTh/PiCluster-API#readme",
  "engines": {
    "node": ">=10.x <=12.x"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "ajv": "^6.12.3",
    "ajv-formats-draft2019": "^1.4.3",
    "archiver": "^5.0.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "csvjson": "^5.1.0",
    "dayjs": "^1.8.32",
    "dockerode": "^3.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-redis-cache": "^1.1.3",
    "express-validator": "^6.6.1",
    "file-type": "^14.7.0",
    "fs-extra": "^9.0.1",
    "helmet": "^4.0.0",
    "hidefile": "^3.0.0",
    "http-status": "^1.4.2",
    "js-yaml": "^3.14.0",
    "jsonstream": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "line-by-line": "^0.1.6",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "papaparse": "^5.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redis": "^3.0.2",
